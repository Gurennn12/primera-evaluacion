def contraseña():
    Password= "Aprobare"
    if(Password.islower and Password.isupper):
        print("Cumple el criterio de contraseña de mayus y  minus")
    else:
        print("no cumple criterios de mayus y minus")
    
        if(Password.isalpha):
            print("No cumple los requisitos de numero")
        else:
            print("cumple los requisitos de numeros")
        

